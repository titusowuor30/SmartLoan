<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anorlddroid.smartloan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartLoan">
        <activity android:name="com.anorlddroid.smartloan.registration.signup.SignUp"/>
        <activity android:name="com.anorlddroid.smartloan.registration.SignIn" />
        <activity android:name="com.anorlddroid.smartloan.onboarding.OnBoardingActivity" />
        <activity android:name="com.anorlddroid.smartloan.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>